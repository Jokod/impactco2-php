{
    "name": "jokod/impactco2-php",
    "description": "Une librairie PHP permettant de comparer la consommation en CO₂e de divers équivalents",
    "keywords": ["impact", "co2", "ademe", "impact carbone", "bilan carbone", "empreinte carbone", "co2e", "équivalent carbone"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "version": "1.0.5",
    "authors": [
        {
            "name": "Jordan.B",
            "email": "jb.jokod@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "monolog/monolog": "^3.7",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "phpro/grumphp": "^2.8"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Jokod\\Impactco2Php\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
